#include <stdio.h>
/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
    int password = 2772;
    int input;
    printf("Enter the password: ");
    scanf("%d", &input);
    if (input == password)
        printf("OK\n");
    else
        printf("Incorrect password\n");
    return 0;
}
